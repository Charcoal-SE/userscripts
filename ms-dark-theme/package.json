{
  "name": "ms-dark-theme",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser",
      "jquery"
    ],
    "rules": {
      "quotes": [
        "warn",
        "double",
        {
          "avoidEscape": true
        }
      ],
      "camelcase": 1,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "dot-notation": 0,
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "unicorn/filename-case": "off",
      "no-var": 0,
      "object-shorthand": 0,
      "unicorn/prefer-node-append": 0
    },
    "globals": [
      "CHAT",
      "Notifier",
      "StackExchange"
    ]
  }
}
