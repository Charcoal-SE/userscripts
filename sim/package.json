{
  "name": "sim",
  "version": "0.0.1",
  "description": "Dig up information on how SmokeDetector handled a post.",
  "scripts": {
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Charcoal-SE/userscripts.git"
  },
  "author": "Charcoal",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "bugs": {
    "url": "https://github.com/Charcoal-SE/userscripts/issues"
  },
  "homepage": "https://github.com/Charcoal-SE/userscripts#readme",
  "devDependencies": {
    "jquery": "^3.5.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser",
      "jquery",
      "node"
    ],
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "camelcase": 0
        }
      }
    ],
    "rules": {
      "quotes": [
        0,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "no-eval": 1,
      "import/no-unassigned-import": 0,
      "camelcase": 1,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "dot-notation": 0,
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "unicorn/filename-case": "off",
      "max-params": [
        1,
        5
      ],
      "unicorn/no-array-instanceof": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "no-var": 0
    }
  }
}
