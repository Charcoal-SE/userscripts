{
  "name": "metapi",
  "version": "0.4.1",
  "description": "A client library for managing cross-userscript metasmoke integration, helping to keep the number of AJAX requests down.",
  "scripts": {
    "test": "xo && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Charcoal-SE/userscripts.git"
  },
  "author": "Charcoal",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "bugs": {
    "url": "https://github.com/Charcoal-SE/userscripts/issues"
  },
  "homepage": "https://github.com/Charcoal-SE/userscripts#readme",
  "devDependencies": {
    "acorn": "^8.6.0",
    "ajv": "^8.8.2",
    "eslint": "^8.18.0",
    "eslint-plugin-promise": "^6.0.0",
    "jquery": "^3.6.0",
    "jsdom": "^9.12.0",
    "mocha": "^9.1.3",
    "require-from-url": "^3.1.3",
    "webpack": "^5.38.1",
    "xo": "^0.50.0"
  },
  "xo": {
    "envs": [
      "browser",
      "jquery",
      "node"
    ],
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "camelcase": 0
        }
      }
    ],
    "rules": {
      "quotes": [
        0,
        "double",
        {
          "avoidEscape": true
        }
      ],
      "no-eval": 1,
      "import/no-unassigned-import": 0,
      "import/extensions": "off",
      "camelcase": 1,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "dot-notation": 0,
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "unicorn/filename-case": "off",
      "max-params": [
        1,
        5
      ],
      "unicorn/no-array-instanceof": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "no-var": 0,
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 0
        }
      ],
      "no-constant-binary-expression": 0,
      "padding-line-between-statements": 0,
      "prefer-destructuring": 0,
      "unicorn/prefer-add-event-listener": 0,
      "valid-jsdoc": 0,
      "ava/no-import-test-files": 0,
      "node/no-deprecated-api": 0,
      "unicorn/no-for-loop": 0,
      "unicorn/prevent-abbreviations": 0,
      "unicorn/prefer-code-point": 0,
      "unicorn/prefer-includes": 0,
      "unicorn/prefer-number-properties": 0,
      "unicorn/no-array-for-each": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-empty-file": 0,
      "unicorn/no-instanceof-array": 0,
      "unicorn/no-this-assignment": 0,
      "unicorn/numeric-separators-style": 0,
      "unicorn/prefer-date-now": 0,
      "unicorn/prefer-dom-node-append": 0,
      "unicorn/prefer-dom-node-remove": 0,
      "unicorn/prefer-module": 0,
      "unicorn/prefer-optional-catch-binding": 0,
      "unicorn/prefer-ternary": 0,
      "unicorn/no-new-array": 0,
      "unicorn/explicit-length-check": 0
    },
    "globals": [
      "metapi"
    ]
  }
}
