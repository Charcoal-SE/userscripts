{
  "name": "metapi",
  "version": "0.4.1",
  "description": "A client library for managing cross-userscript metasmoke integration, helping to keep the number of AJAX requests down.",
  "scripts": {
    "test": "xo && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Charcoal-SE/userscripts.git"
  },
  "author": "Charcoal",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "bugs": {
    "url": "https://github.com/Charcoal-SE/userscripts/issues"
  },
  "homepage": "https://github.com/Charcoal-SE/userscripts#readme",
  "devDependencies": {
    "jquery": "^3.2.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "require-from-url": "^3.0.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser",
      "jquery",
      "node"
    ],
    "rules": {
      "quotes": [
        0,
        "double",
        {
          "avoidEscape": true
        }
      ],
      "no-eval": 1,
      "import/no-unassigned-import": 0,
      "no-global-assign": 0,
      "camelcase": 1,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "dot-notation": 0,
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "unicorn/filename-case": "off"
    },
    "globals": [
      "metapi"
    ]
  }
}
